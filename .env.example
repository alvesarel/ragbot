# ===========================================
# Clinic AI Agents - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# ===========================================
# EVOLUTION API SETTINGS
# ===========================================

# Evolution API URL (where Evolution API is running)
EVOLUTION_API_URL=http://localhost:8080

# API Key for authenticating with Evolution API
# Generate a secure random string (e.g., openssl rand -hex 32)
EVOLUTION_API_KEY=your-secure-api-key-here

# Instance name (will be created on first connection)
EVOLUTION_INSTANCE_NAME=clinic-whatsapp

# ===========================================
# N8N WEBHOOK SETTINGS
# ===========================================

# Your n8n instance URL (must be accessible from Evolution API)
# For local development: http://host.docker.internal:5678 (Docker)
# For production: https://your-n8n-domain.com
N8N_WEBHOOK_URL=http://localhost:5678/webhook

# ===========================================
# TEAM CONFIGURATION
# ===========================================

# Admin phone number for team notifications (format: country code + number, no +)
# Example: 5511999998888
ADMIN_PHONE_NUMBER=5511999998888

# Authorized phone numbers for Yara access (comma-separated)
# Example: 5511999998888,5511888887777,5521999996666
AUTHORIZED_PHONES=5511999998888

# ===========================================
# GOOGLE SHEETS
# ===========================================

# Patients Sheet ID (for Diana patient lookup)
PATIENTS_SHEET_ID=your-google-sheet-id

# ===========================================
# EMBEDDING SCRIPT (RAG Knowledge Base)
# ===========================================
# Used by: node scripts/embed_knowledge_base.js

# OpenAI API Key (for embeddings)
OPENAI_API_KEY=sk-xxxxx

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key
QDRANT_COLLECTION_NAME=clinic_knowledge_base

# ===========================================
# N8N CREDENTIALS (Configure in n8n UI)
# ===========================================
# These are configured as n8n credentials, NOT env vars:
# - Anthropic API (Claude Haiku 4.5)
# - OpenAI API (embeddings + transcription)
# - Qdrant API
# - Evolution API
# - Telegram Bot API
# - Google Sheets OAuth

# ===========================================
# APPLICATION SETTINGS
# ===========================================
TIMEZONE=America/Sao_Paulo
